prefix=/Library/Frameworks/GStreamer.framework/Versions/1.0
exec_prefix=${prefix}
libdir=/Library/Frameworks/GStreamer.framework/Versions/1.0/lib
includedir=${prefix}/include

Name: GnuTLS
Description: Transport Security Layer implementation for the GNU system
URL: http://www.gnutls.org/
Version: 3.3.14
Libs: -L${libdir} -lgnutls
Libs.private: -L/Library/Frameworks/GStreamer.framework/Versions/1.0/lib -lz -R/Library/Frameworks/GStreamer.framework/Versions/1.0/lib -L/Library/Frameworks/GStreamer.framework/Versions/1.0/lib -lintl -L/Library/Frameworks/GStreamer.framework/Versions/1.0/lib -liconv -lc -R/Library/Frameworks/GStreamer.framework/Versions/1.0/lib -Wl,-framework -Wl,CoreFoundation   -L/Library/Frameworks/GStreamer.framework/Versions/1.0/lib -liconv -R/Library/Frameworks/GStreamer.framework/Versions/1.0/lib    -lgmp
Requires.private: nettle, hogweed, libtasn1, zlib
Cflags: -I${includedir}
